# The endpoint for setting a new password.
url = "http://localhost:8080/index.php/new_password/save"

# Send previously saved cookies.
cookie = cookiefile
# Save returned cookies.
cookie-jar = cookiefile
# Divert output to file.
output = outputfile
# Don't show progress.
silent
# Do show errors though.
show-error

# Enable debugging so you can set breakpoints in the PHP.
data = "XDEBUG_SESSION=true"

# The information we want to post.
data = "id=1"
data = "email_key="
data = "username=admin"
data = "email_address=admin@example.com"
data = "password=password"
data = "password2=password"
